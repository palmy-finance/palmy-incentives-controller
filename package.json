{
  "name": "incentives-controller",
  "version": "1.0.0",
  "description": "Incentives controller for Palmy protocol",
  "files": [
    "contracts",
    "artifacts"
  ],
  "scripts": {
    "run-env": "npm i && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat:kovan": "hardhat --network kovan",
    "hardhat:rinkeby": "hardhat --network rinkeby",
    "hardhat:tenderly": "hardhat --network tenderly",
    "hardhat:main": "hardhat --network main",
    "hardhat:docker": "hardhat --network hardhatevm_docker",
    "hardhat:oasys": "hardhat --network oasys",
    "hardhat:testnet": "hardhat --network testnet",
    "coverage": "hardhat coverage",
    "test": "npm run test-pull-rewards-incentives && npm run test-staked-incentives",
    "test-pull-rewards-incentives": "TS_NODE_TRANSPILE_ONLY=1 hardhat test ./test/__setup.spec.ts ./test/PullRewardsIncentivesController/*.spec.ts",
    "test-staked-incentives": "TS_NODE_TRANSPILE_ONLY=1 hardhat test ./test/__setup.spec.ts ./test/StakedIncentivesController/*.spec.ts",
    "test-proposal": "TS_NODE_TRANSPILE_ONLY=1 MAINNET_FORK=true hardhat test ./test-fork/incentivesProposal.spec.ts",
    "test-proposal:tenderly": "TS_NODE_TRANSPILE_ONLY=1 TENDERLY=true npm run hardhat:tenderly -- test ./test-fork/incentivesProposal.spec.ts",
    "test-proposal-skip": "TS_NODE_TRANSPILE_ONLY=1 MAINNET_FORK=true hardhat test ./test-fork/incentives-skip.spec.ts",
    "test-proposal-skip:tenderly": "TS_NODE_TRANSPILE_ONLY=1 TENDERLY=true npm run hardhat:tenderly -- test ./test-fork/incentives-skip.spec.ts",
    "spin-incentives:tenderly": "TENDERLY=true npx hardhat --network tenderly incentives-proposal:tenderly",
    "compile": "SKIP_LOAD=true hardhat compile",
    "console:fork": "MAINNET_FORK=true hardhat console",
    "prepublishOnly": "npm run compile",
    "oasys:deployment": "npm run hardhat:oasys -- common-deployment",
    "testnet:deployment": "npm run hardhat:testnet -- common-deployment",
    "oasys:initialization": "npm run hardhat:oasys -- common-initialization",
    "testnet:initialization": "npm run hardhat:testnet -- common-initialization",
    "oasys:verification": "npm run hardhat:oasys -- verification",
    "testnet:verification": "npm run hardhat:testnet -- verification",
    "print-configs:oasys": "npm run hardhat:oasys -- print-configs",
    "print-configs:testnet": "npm run hardhat:testnet -- print-configs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palmy-finance/palmy-incentives-controller.git"
  },
  "author": "Palmy finance",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/palmy-finance/palmy-incentives-controller/issues"
  },
  "homepage": "https://github.com/palmy-finance/palmy-incentives-controller#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "3.1.0",
    "@tenderly/hardhat-tenderly": "^1.1.0-beta.5",
    "@typechain/ethers-v5": "^6.0.5",
    "@typechain/hardhat": "^1.0.1",
    "@types/chai": "4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.5",
    "bignumber.js": "9.0.0",
    "chai": "4.2.0",
    "chai-bignumber": "3.0.0",
    "chai-bn": "^0.2.1",
    "defender-relay-client": "^1.5.1",
    "dotenv": "^8.2.0",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "3.0.2",
    "ethereumjs-util": "7.0.2",
    "ethers": "^5.0.19",
    "globby": "^11.0.1",
    "hardhat": "^2.1.2",
    "hardhat-gas-reporter": "^1.0.0",
    "husky": "^4.2.5",
    "is-ipfs": "^5.0.0",
    "kebab-case": "^1.0.1",
    "lowdb": "1.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.53",
    "pretty-quick": "^2.0.1",
    "solidity-coverage": "^0.7.16",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "ts-generator": "^0.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "^4.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "hardhat-deploy": "^0.7.2",
    "tmp-promise": "^3.0.3"
  }
}
